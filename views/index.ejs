<!DOCTYPE html>
<html lang="en">
    <%- include('layouts/head'); -%>
    <body>
        <%- include('layouts/navigation'); -%>
        <!-- Page Header-->
        <header class="masthead" style="background-image: url('assets/img/home-bg.jpg')">
            <div class="container position-relative px-4 px-lg-5">
                <div class="row gx-4 gx-lg-5 justify-content-center">
                    <div class="col-md-10 col-lg-8 col-xl-7">
                        <div class="site-heading">
                            <h1>Dashboard</h1>
                            <span class="subheading">DriveTest Booking Kiosk</span>
                        </div>
                    </div>
                </div>
            </div>
        </header>
        <!-- Main Content-->
        <div class="container px-4 px-lg-5">
            <% if(typeof success !='undefined' ) { %>
                <div class="row gx-4 gx-lg-5 justify-content-center">
                    <p class="text-center text-success">
                        <%= success %>
                    </p>
                    </div>
                </div>
            <% } %>
            <div class="row gx-4 gx-lg-5 justify-content-center">
                <div class="col-md-10 col-lg-8 col-xl-7">
                    <!-- Post preview-->
                    <div class="post-preview">
                        <a href="post.html">
                            <h2 class="post-title">You can book G2/G drive test from this Kiosk</h2>
                            <h3 class="post-subtitle">You can book G2/G drive test from this Kiosk</h3>
                        </a>
                        <p class="post-meta">
                            Powered by DriveTest Center
                        </p>
                    </div>
                    <!-- Divider-->
                    <hr class="my-4" />
                </div>
            </div>
        </div>
        <%- include('layouts/footer'); -%>
    </body>
</html>
